<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="member">
 <insert id="add" parameterType="mvo">
	insert into member 
	values(member_seq.nextVal,#{id},#{pwd},#{name},#{age},
	#{gender},sysdate)
</insert>

<select id="idchk" parameterType="String" resultType="int">
	select count(*) from member where id=#{id}
</select>

<!-- login mapper -->
<select id="login" parameterType="mvo" resultType="mvo">
   select id, name from member where id=#{id} and pwd=#{pwd}
</select>

<!-- login loggin mapper -->
<insert id="logger_in" parameterType="lvo">
insert into myloginlog values(myloginlog_seq.nextVal,#{idn},#{reip},
		#{uagent},#{status},sysdate)
</insert>



















 </mapper>